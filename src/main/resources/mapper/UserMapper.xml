<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hxci.mapper.UserMapper">

  <select id="query" resultType="com.hxci.pojo.User">
    select * from user
  </select>

  <insert id="add">
    insert into user (name,age,bid)values (#{name},#{age},#{bid})
  </insert>

  <delete id="delete">
    delete from user where id = #{id}
  </delete>
<!--?-->
</mapper>
